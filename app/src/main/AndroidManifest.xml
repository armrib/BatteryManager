<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aware.plugin.batteryManager" >

    <uses-permission android:name="android.permission.BATTERY_STATS" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name">

        <service
            android:name="com.aware.plugin.batteryManager.Plugin"
            android:exported="true" />
        <service
            android:exported="true"
            android:name="com.aware.plugin.batteryManager.Plugin$Bat_Service"/>
        <service android:name="com.aware.Aware"/>
        <receiver android:name="BatteryAlarm"/>
        <activity
            android:name="com.aware.plugin.batteryManager.Settings"
            android:exported="true" />
        <provider android:name="com.aware.plugin.batteryManager.Provider"
            android:authorities="${applicationId}.provider.batteryManager"
            android:exported="true"
            android:readPermission="com.aware.READ_CONTEXT_DATA"
            android:writePermission="com.aware.WRITE_CONTEXT_DATA">
        </provider>
    </application>

</manifest>
